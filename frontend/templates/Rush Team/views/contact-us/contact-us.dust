                    <div class="box_of_contact_us">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-6">
                                    <form class="form_of_contact_us dir_rtl" autocomplete="off">
                                        <div class="form-group  d-inline-block  inline_form_class">
                                            <label for="fn" class="label_of_form">
                                                نام
                                                <span class="should_be_fill">*</span>
                                            </label>
                                            <input type="text" class="form-control focus_on_form" id="fn">
                                        </div>
                                        <div class="form-group d-inline-block inline_form_class m_r_form_inline">
                                            <label for="ln" class="label_of_form">
                                                نام خانوادگی
                                                <span class="should_be_fill">*</span>
                                            </label>
                                            <input type="text" class="form-control focus_on_form" id="ln">
                                        </div>
                                        <div class="form-group">
                                            <label for="email" class="label_of_form">
                                                ایمیل
                                                <span class="should_be_fill">*</span>
                                            </label>
                                            <input type="email" class="form-control focus_on_form" id="email">
                                        </div>
                                        <div class="form-group">
                                            <label for="message_title" class="label_of_form">
                                                عنوان پیام
                                                <span class="should_be_fill">*</span>
                                            </label>
                                            <input type="text" class="form-control focus_on_form" id="message_title">
                                        </div>
                                        <div class="form-group">
                                            <label for="textarea" class="label_of_form">
                                                متن پیام
                                                <span class="should_be_fill">*</span>
                                            </label>
                                            <textarea class="form-control focus_on_form" id="textarea"
                                                      rows="3"></textarea>
                                        </div>
                                        <button type="submit" class="btn w-100 mt-2 btn_of_contact_us">ارسال</button>
                                        <hr class="mt-4">
                                        <div class="contact_information">
                                            <h3 class="contact_information_title w-100 text-right ">
                                                اطلاعات تماس
                                            </h3>
                                            <div class="location_of_us  contact_information_us text-right mb-3">
                                                <i class="fas fa-map-marker-smile ml-2 font_size_of_icon_in_contact_information"></i>
                                                <span class="text_of_contact_information">خونه کسری اینا</span>
                                            </div>
                                            <div class="contact_of_us contact_information_us text-right mb-3">
                                                <i class="fas fa-phone ml-2 font_size_of_icon_in_contact_information"></i>
                                                <span class="text_of_contact_information">09021414866</span>
                                            </div>
                                            <div class="email_of_us contact_information_us text-right">
                                                <i class="fas fa-envelope ml-2 font_size_of_icon_in_contact_information"></i>
                                                <span class="text_of_contact_information">RushTeam.web@gmail.com</span>
                                            </div>
                                        </div>
                                    </form>

